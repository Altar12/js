    "packages/js-core": {
      "root": "packages/js-core",
      "projectType": "library",
      "sourceRoot": "@",
      "targets": {
        "prebuild": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn depcheck --ignore-patterns=test --ignores=buffer",
            "cwd": "packages/js-core"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "commands": ["yarn workspace @metaplex-foundation/js-core build"],
            "parallel": false
          },
          "dependsOn": [
            {
              "target": "prebuild",
              "projects": "self"
            },
            {
              "target": "build",
              "projects": "dependencies"
            }
          ]
        },
        "clean": {
          "executor": "nx:run-commands",
          "options": {
            "commands": ["yarn workspace @metaplex-foundation/js-core clean"],
            "parallel": false
          }
        },
        "publish": {
          "executor": "nx:run-commands",
          "options": {
            "command": "node ./scripts/publish.mjs packages/js-core {args.ver} {args.tag}",
            "forwardAllArgs": true
          },
          "dependsOn": [
            {
              "projects": "self",
              "target": "build"
            }
          ]
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/js-core/**/*.ts"],
            "fix": true
          }
        },
        "test": {
          "executor": "nx:run-commands",
          "options": {
            "commands": ["yarn workspace @metaplex-foundation/js-core test"],
            "parallel": false
          },
          "dependsOn": [
            {
              "target": "build",
              "projects": "self"
            }
          ]
        }
      },
      "tags": []
    },
