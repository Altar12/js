{
  "name": "@metaplex-foundation/js-plugin-derived-identity-module",
  "version": "0.1.1",
  "sideEffects": false,
  "module": "dist/esm/index.mjs",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "/dist/cjs",
    "/dist/esm",
    "/dist/types",
    "/src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "description": "Metaplex JavaScript SDK",
  "keywords": [
    "nft",
    "metaplex",
    "solana",
    "blockchain",
    "plugin",
    "nft.storage"
  ],
  "author": "Metaplex Maintainers <contact@metaplex.com>",
  "homepage": "https://metaplex.com",
  "repository": {
    "url": "https://github.com/metaplex-foundation/js.git"
  },
  "scripts": {
    "lint:test": "yarn eslint --ext js,ts,tsx src",
    "lint:fix": "yarn eslint --fix --ext js,ts,tsx src",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc && tsc-alias && tsc -p test/tsconfig.json && tsc-alias -p test/tsconfig.json && rollup -c",
    "test": "tape dist/test/**/*.test.js",
    "preversion": "yarn build && yarn test",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
   "@metaplex-foundation/js-core": "*",    "@bundlr-network/client": "^0.8.8",
    "@metaplex-foundation/js": "*",
    "@metaplex-foundation/mpl-candy-machine": "^4.6.0",    "@metaplex-foundation/mpl-auction-house": "^2.1.1",
    "@metaplex-foundation/mpl-token-metadata": "^2.3.2",
    "@noble/ed25519": "^1.7.1",
    "@noble/hashes": "^1.1.3",
    "@solana/web3.js": "^1.47.3",
    "@solana/spl-token": "^0.3.5",
    "abort-controller": "^3.0.0",
    "bn.js": "^5.2.1",
    "debug": "^4.3.4",
    "eventemitter3": "^4.0.7"
  },
  "engines": {
    "node": ">=16.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "js-plugin-nft-storage"
  }
}
