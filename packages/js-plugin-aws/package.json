    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/debug": "^4.1.7",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isequal": "^4.5.6",
    "@types/mime": "^2.0.3",
    "@types/sinon": "^10.0.11",
    "@types/tape": "^4.13.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "nx": "^14.4.3",
    "prettier": "^2.6.0",
