    "rollup-plugin-polyfill-node": "^0.9.0",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^13.0.1",
    "spok": "^1.4.3",
    "tape": "^5.5.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^14.0 || >=16.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
